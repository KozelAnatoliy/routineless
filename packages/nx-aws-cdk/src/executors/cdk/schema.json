{
  "$schema": "http://json-schema.org/schema",
  "title": "Cdk executor",
  "description": "Executor for aws cdk",
  "type": "object",
  "properties": {
    "account": {
      "description": "AWS account id",
      "type": "number"
    },
    "region": {
      "description": "AWS region",
      "type": "string"
    },
    "watch": {
      "description": "Watch mode. Will trigger the command when files changes.",
      "type": "boolean"
    }
  }
}
