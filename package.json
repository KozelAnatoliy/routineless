{
  "name": "routineless",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build",
    "e2e": "nx run-many --target=e2e",
    "lint": "nx run-many --target=lint",
    "publish:local": "nx build nx-plugin && cd dist/packages/nx-plugin && (npm unpublish @routineless/nx-plugin --force --registry http://localhost:4873 || true) && npm publish --tag=latest --access public --registry http://localhost:4873",
    "test": "nx run-many --target=test",
    "test:coverage": "nx run-many --target=test --codeCoverage=true --output-style=\"static\" --passWithNoTests=false --skip-nx-cache"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@nx/devkit": "16.9.0",
    "@nx/eslint-plugin": "16.9.0",
    "@nx/jest": "16.9.0",
    "@nx/js": "16.9.0",
    "@nx/linter": "16.9.0",
    "@nx/node": "16.9.0",
    "@nx/plugin": "16.9.0",
    "@nx/workspace": "16.9.0",
    "@swc-node/register": "^1.6.3",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.88",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@tsconfig/node-lts": "^18.12.1",
    "@tsconfig/strictest": "^2.0.0",
    "@types/eslint": "^8.44.3",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cdk": "^1.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsonc-eslint-parser": "^2.2.0",
    "nx": "16.9.0",
    "nx-cloud": "16.4.0",
    "prettier": "^2.8.7",
    "ts-jest": "29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "typescript-transform-paths": "^3.4.6"
  }
}
