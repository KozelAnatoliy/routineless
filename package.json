{
  "name": "routineless",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --target=build --all",
    "e2e": "nx run-many --target=e2e --all",
    "lint": "nx run nx-plugin:lint",
    "publish:local": "nx build nx-plugin && cd dist/packages/nx-plugin && (npm unpublish @routineless/nx-plugin --force --registry http://localhost:4873 || true) && npm publish --tag=latest --access public --registry http://localhost:4873",
    "test": "nx run-many --target=test --all",
    "test:coverage": "nx run-many --target=test --all --codeCoverage=true --output-style=\"static\" --passWithNoTests=false"
  },
  "private": true,
  "dependencies": {
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@nrwl/cli": "^15.9.2",
    "@nrwl/devkit": "^15.9.2",
    "@nrwl/eslint-plugin-nx": "^15.9.2",
    "@nrwl/jest": "^15.9.2",
    "@nrwl/js": "^15.9.2",
    "@nrwl/linter": "^15.9.2",
    "@nrwl/node": "^15.9.2",
    "@nrwl/nx-cloud": "^15.3.5",
    "@nrwl/nx-plugin": "^15.9.2",
    "@nrwl/workspace": "^15.9.2",
    "@swc-node/register": "^1.6.3",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.46",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@tsconfig/node-lts": "^18.12.1",
    "@tsconfig/strictest": "^2.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cdk": "^1.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jsonc-eslint-parser": "^2.2.0",
    "nx": "^15.9.2",
    "prettier": "^2.8.7",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3",
    "typescript-transform-paths": "^3.4.6"
  }
}
